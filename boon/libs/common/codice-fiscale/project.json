{
  "name": "common-codice-fiscale",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/common/codice-fiscale/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/common/codice-fiscale",
        "main": "libs/common/codice-fiscale/src/index.ts",
        "tsConfig": "libs/common/codice-fiscale/tsconfig.lib.json",
        "assets": ["libs/common/codice-fiscale/*.md"],
        "updateBuildableProjectDepsInPackageJson": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/common/codice-fiscale/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/common/codice-fiscale/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
